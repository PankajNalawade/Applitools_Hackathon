{
  "name": "applifashion",
  "version": "1.0.0",
  "description": "Applitools Hackathon",
  "main": "index.js",
  "scripts": {
    "cy-open": "cypress open",
    "test-chrome-TraditionalTestsV1": "cypress run --spec cypress/integration/TraditionalTestsV1.js -b chrome --headless -r mochawesome --reporter-options reportDir=cypress/test_reports,reportFilename=Traditional-V1-TestResults",
    "test-chrome-TraditionalTestsV2": "cypress run --spec cypress/integration/TraditionalTestsV2.js -b chrome --headless -r mochawesome --reporter-options reportDir=cypress/test_reports,reportFilename=Traditional-V2-TestResults",
    "test-ModernTestsV1": "cypress run --spec cypress/integration/ModernTestsV1.js -b chrome --headless",
    "test-ModernTestsV2": "cypress run --spec cypress/integration/ModernTestsV2.js -b chrome --headless"
  },
  "author": "Team APN",
